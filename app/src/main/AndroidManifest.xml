<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.schwert398.cerkenoter">

    <application
        android:allowBackup="true"
        android:icon="@drawable/cerke_noter_icon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/cerke_noter_icon"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity
            android:name="com.schwert398.cerkenoter.MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.schwert398.cerkenoter.SetDataActivity"
            android:parentActivityName="com.schwert398.cerkenoter.MainActivity"
            android:screenOrientation="portrait">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.schwert398.cerkenoter.MainActivity" />
        </activity>

    </application>

</manifest>